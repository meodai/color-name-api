  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.0"
  },
  "bugs": {
    "url": "https://github.com/meodai/color-name-api/issues"
  },
  "homepage": "https://github.com/meodai/color-name-api#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.10",
    "color-name-list": "^11.3.0",
    "color-name-lists": "^3.27.0",
    "culori": "^4.0.1",
    "dotenv": "^16.4.7",
    "request-ip": "^3.3.0",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.8.1"
